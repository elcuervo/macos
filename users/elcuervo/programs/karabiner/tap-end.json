{
  "title": "Tap left shift for Home and right to End",
  "rules": [
    {
      "description": "Shift_L tap -> Home, Shift_R tap -> End",
      "manipulators": [
        {
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": {
            "key_code": "left_shift"
          },
          "to_if_alone": [
            { "key_code": "home" }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": {
            "key_code": "right_shift"
          },
          "to_if_alone": [
            { "key_code": "end" }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
